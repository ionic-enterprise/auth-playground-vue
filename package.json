{
  "name": "auth-playground-vue",
  "description": "Identity Vault and Auth Connect Playground",
  "version": "0.0.1",
  "private": true,
  "author": "Ionic Framework",
  "scripts": {
    "assets": "capacitor-assets generate --iconBackgroundColor '#f1ebe1' --splashBackgroundColor '#f1ebe1' --iconBackgroundColorDark '#110b00' --splashBackgroundColorDark '#110b00'",
    "build": "vue-tsc && vite build && cap sync",
    "build:web": "vue-tsc && vite build",
    "dev": "vite",
    "lint": "eslint",
    "prepare": "husky install",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "@capacitor/android": "^5.0.0",
    "@capacitor/app": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/haptics": "^5.0.0",
    "@capacitor/ios": "^5.0.0",
    "@capacitor/keyboard": "^5.0.0",
    "@capacitor/preferences": "^5.0.0",
    "@capacitor/status-bar": "^5.0.0",
    "@ionic-enterprise/auth": "~5.0.1",
    "@ionic-enterprise/identity-vault": "~5.11.1",
    "@ionic/vue": "~7.2.0",
    "@ionic/vue-router": "~7.2.0",
    "@vitest/coverage-v8": "~0.33.0",
    "axios": "~1.4.0",
    "vee-validate": "~4.10.6",
    "vue": "^3.2.21",
    "vue-router": "^4.0.12",
    "yup": "~1.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.0",
    "@ionic/prettier-config": "~4.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "cypress": "~12.17.1",
    "eslint": "^8.4.1",
    "eslint-plugin-vue": "~9.15.1",
    "husky": "~8.0.1",
    "jsdom": "~22.1.0",
    "lint-staged": "~13.2.3",
    "prettier": "~3.0.0",
    "typescript": "~5.1.6",
    "vite": "^4.3.9",
    "vitest": "~0.33.0",
    "vue-tsc": "^1.0.24",
    "wait-for-expect": "~3.0.2"
  },
  "prettier": "@ionic/prettier-config"
}
