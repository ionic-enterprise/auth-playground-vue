{
  "name": "auth-playground-vue",
  "description": "Identity Vault and Auth Connect Playground",
  "version": "0.0.1",
  "private": true,
  "author": "Ionic Framework",
  "scripts": {
    "assets": "capacitor-assets generate --iconBackgroundColor '#f1ebe1' --splashBackgroundColor '#f1ebe1' --iconBackgroundColorDark '#110b00' --splashBackgroundColorDark '#110b00'",
    "build": "vue-tsc && vite build && cap sync",
    "dev": "vite",
    "lint": "eslint",
    "prepare": "husky install",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "@capacitor/android": "~4.7.0",
    "@capacitor/app": "^4.0.1",
    "@capacitor/core": "^4.0.1",
    "@capacitor/haptics": "^4.0.1",
    "@capacitor/ios": "~4.7.0",
    "@capacitor/keyboard": "^4.0.1",
    "@capacitor/preferences": "~4.0.1",
    "@capacitor/status-bar": "^4.0.1",
    "@ionic-enterprise/auth": "~4.0.0-dev-20221130T170252.0",
    "@ionic-enterprise/identity-vault": "~5.9.4",
    "@ionic/vue": "~7.0.0-beta.1",
    "@ionic/vue-router": "~7.0.0-beta.1",
    "axios": "~1.3.1",
    "vee-validate": "~4.8.4",
    "vue": "^3.2.21",
    "vue-router": "^4.0.12",
    "yup": "~1.0.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^4.0.1",
    "@ionic/prettier-config": "~2.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "~0.29.7",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "cypress": "~12.8.1",
    "eslint": "^8.4.1",
    "eslint-plugin-vue": "~9.10.0",
    "husky": "~8.0.1",
    "jsdom": "^21.1.0",
    "prettier": "~2.8.0",
    "pretty-quick": "~3.1.3",
    "typescript": "^4.3.5",
    "vite": "^4.1.0",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.0.24",
    "wait-for-expect": "~3.0.2"
  },
  "prettier": "@ionic/prettier-config"
}
